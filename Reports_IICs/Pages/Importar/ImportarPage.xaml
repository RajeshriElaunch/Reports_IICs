<UserControl x:Class="Reports_IICs.Pages.Importar.ImportarPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Pages/BlankPage.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Instrumentos" Source="/Pages/Importar/ImportarInstrumentos.xaml" />
                <mui:Link DisplayName="Bloomberg" Source="/Pages/Importar/ImportarBloomberg.xaml" />
                <mui:Link x:Name="LinkCategorias" DisplayName="Categorías" Source="/Pages/ListadoPrueba.xaml#Instrumentos_Categorias"  />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
