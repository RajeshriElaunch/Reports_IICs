<UserControl x:Class="Reports_IICs.Pages.Secciones.SeccionesPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Name="ModernTabSecciones" Layout="List" SelectedSource="/Pages/BlankPage.xaml" SelectedSourceChanged="ModernTabSecciones_SelectedSourceChanged">
            <mui:ModernTab.Links>
                <mui:Link x:Uid="0"  DisplayName="Listado" Source="/Pages/ListadoPrueba.xaml"  />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
